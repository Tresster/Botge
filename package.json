    "bot",
    "discord",
    "discord-bot",
    "emotes"
  ],
  "homepage": "https://botge.gitbook.io",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Tresster/Botge.git"
  },
  "license": "MIT",
  "author": {
    "name": "Tresster"
  },
  "contributors": [
    {
      "name": "gentlebob"
    },
    {
      "name": "widepogcrazy"
    },
    {
      "name": "TimotronPrime"
    },
    {
      "name": "santirack"
    }
  ],
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "commandgeupdate": "tsc && node dist/update-commands.js",
    "lint": "eslint src",
    "prepare": "husky || true",
    "prettier-format": "prettier --config .prettierrc . --write",
    "start": "tsc && node dist/index.js",
