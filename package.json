{
  "name": "botge",
  "version": "2.8.1",
  "description": "Search emotes, clips, use zero-width emotes and other such commands.",
  "keywords": [
    "bot",
    "discord",
    "discord-bot",
    "emotes"
  ],
  "homepage": "https://botge.gitbook.io",
  "bugs": {
    "url": "https://github.com/Tresster/Botge/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Tresster/Botge.git"
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/Tresster"
  },
  "license": "MIT",
  "author": "Tresster",
  "contributors": [
    "gentlebob",
    "widepogcrazy",
    "TimotronPrime",
    "santirack"
  ],
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc --build src",
    "build:clean": "tsc --build --clean src",
    "build:production": "tsc --project src/tsconfig.production.json",
    "eslint": "eslint . --flag unstable_native_nodejs_ts_config --concurrency=auto",
    "prepare": "node .husky/install.ts",
    "prettier": "prettier . --write",
    "start": "docker image prune --force && docker-compose up --build",
    "stop": "docker compose stop",
    "test": "vitest tests"
  },
  "dependencies": {
    "@discordjs/voice": "^0.19.0",
    "@google/genai": "1.34.0",
    "@twemoji/api": "^17.0.2",
    "deepl-node": "^1.24.0",
    "discord.js": "^14.25.1",
    "fs-extra": "^11.3.3",
    "meilisearch": "^0.54.0",
    "node-fetch": "^3.3.2",
    "node-schedule": "^2.1.1",
    "openai": "^6.15.0",
    "sql.js": "1.13.0",
    "timezones-list": "^3.1.0"
  },
  "devDependencies": {
    "@eslint/js": "9.39.2",
    "@microsoft/tsdoc-config": "0.18.0",
    "@types/fs-extra": "^11.0.4",
    "@types/node-schedule": "^2.1.8",
    "@types/sql.js": "1.4.9",
    "eslint": "9.39.2",
    "eslint-plugin-tsdoc": "0.5.0",
    "husky": "^9.1.7",
    "markdownlint": "0.40.0",
    "prettier": "^3.7.4",
    "prettier-plugin-sh": "0.18.0",
    "ts-essentials": "^10.1.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.50.0",
    "vite-tsconfig-paths": "6.0.3",
    "vitest": "^4.0.16"
  },
  "engines": {
    "node": "v25.2.1",
    "npm": ">=11.6.2"
  },
  "os": [
    "linux",
    "darwin",
    "win32"
  ],
  "cpu": [
    "x64",
    "arm64"
  ]
}
