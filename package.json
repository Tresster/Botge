    "deepl-node": "^1.19.1",
    "discord.js": "^14.22.1",
    "dotenv": "^17.2.2",
    "fs-extra": "^11.3.2",
    "meilisearch": "^0.53.0",
    "node-fetch": "^3.3.2",
    "node-schedule": "^2.1.1",
    "openai": "^5.22.0",
    "ts-md5": "^1.3.1"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/fs-extra": "^11.0.4",
    "@types/node-schedule": "^2.1.8",
    "eslint": "^9.36.0",
    "husky": "^9.1.7",
