{
  "compilerOptions": {
    "target": "esnext",
    "module": "nodenext",
    "sourceMap": false,
    "outDir": "dist",
    "strict": true,
    "exactOptionalPropertyTypes": true,
    "noImplicitReturns": true,
    "noPropertyAccessFromIndexSignature": true,
    "noUncheckedSideEffectImports": true,
    "rewriteRelativeImportExtensions": true
  },
  "include": ["src"]
}
